<h2 class="page-header">Dashboard</h2>

<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

<script language="Javascript" type="text/javascript">
   $.ajax({
                  url: "/state/aarushi",
                  // Handle as Text
                  dataType: "text",
                  success: function(data) 
                  {
                      // Parse JSON file
                      var json = $.parseJSON(data);


                      //Store data into a variable
                      // Display Players
                      for(var i=1;i<=Number(json.value)+1;i++ )
                      $("#"+i).prop('disabled', false);
                       console.log("here"+Number(json.value));
                     // $('#results').html('Currently there are: ' + json.value ' users');
                       $('.div_image').html('<img src="json.url" />'); 
                   }
      });
</script>

<p>Welcome to your dashboard</p>  
<br>


<h2 class="page-header">Student Result</h2>
{{#if errors}}
      {{#each errors}}
        <div class="alert alert-danger">{{msg}}</div>
      {{/each}}
{{/if}}

<form method="post" action="/newquery">
  <div class="form-group">
    <label>Roll No</label>
    <input type="text" class="form-control" placeholder="Roll_No" name="Roll_No">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>
